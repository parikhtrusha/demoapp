<h2 style="text-align:center;">Sign In Form</h2>
<div class="hero-unit">
<div>
<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <tabel>
  <tr><td><label class="control-label">Email</label></td>
  <td><%= f.email_field :email %></td></tr>
  

  <tr><td><label class="control-label">Password</label></td>
  <td><%= f.password_field :password %></td></tr>

  <% if devise_mapping.rememberable? -%>
    <br><tr><td><%= f.check_box :remember_me %><%= f.label :remember_me %></td></tr>
  <% end -%>

  <tr><%= f.submit "Sign in" %></tr>
<% end %>
</tabel>
<%= render :partial => "devise/shared/links" %>
</div>
</div>